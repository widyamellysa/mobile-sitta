    <style name="EditText"/>

    <style name="EditText.PoppinsNormal14sp8D92A3">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">#8D92A3</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Button"/>

    <style name="Button.Primary">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">#202020</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/shape_rectangle_8_ffc700</item>
    </style>

    <style name="Button.Secondary">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">#FFF</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/shape_rectangle_8_8d92a3</item>
    </style>

    <style name="Button.Cancelled">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/shape_rectangle_8_d9435e</item>
    </style>

    <style name="Tab">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">#8D92A3</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="RatingBar">
        <item name="colorControlNormal">#ECECEC</item>
        <item name="colorControlActivated">#FFC700</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <!-- The launcher theme. It sets the main window background to the launch_screen drawable -->
    <!--styles for app-->
    <!--df dialog-->
    <style name="viewParent">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="viewParent.headerText">
        <item name="android:textColor">@color/contentTextColor</item>
        <item name="android:textSize">@dimen/headerTextSize</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="viewParent.headerText.NoMode">
        <item name="android:textColor">@color/darkTextColor</item>
    </style>
    <style name="parent">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="modifiedEditText" parent="parent">
        <item name="android:textSize">@dimen/newsMoreTextSize</item>
    </style>
    <style name="loginButton" parent="viewParent">
        <item name="android:background">@drawable/button_background_blue_ui_design</item>
        <item name="android:textColor">@color/whiteTextColor</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="spinnerHintText" parent="parent">
        <item name="android:textSize">@dimen/newsMoreTextSize</item>
        <item name="android:textColor">@color/colorAccentForUiDesign</item>
        <item name="android:paddingLeft">5dp</item>
    </style>
    <!--added for faculty login design-->
    <!--designs login_bkogin and register-->
    <style name="parentFacultyLogin">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="loginParentFacultyLogin" parent="parent">
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/loginBkColorFacultyLogin</item>
        <item name="android:focusableInTouchMode">true</item>
    </style>
    <style name="headerTextFacultyLogin" parent="parent">
        <item name="android:textSize">@dimen/headerTextSizeFacultyLogin</item>
        <item name="android:textColor">@color/whiteTextColorFacultyLogin</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="loginHeaderFacultyLogin">
        <item name="android:textColor">@color/primaryTextColorFacultyLogin</item>
        <item name="android:textSize">@dimen/headerTextSizeFacultyLogin</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="loginCardFacultyLogin" parent="parent"></style>
    <style name="linearParenFacultyLogint" parent="parent">
        <item name="android:orientation">vertical</item>
    </style>
    <style name="modifiedEditTextFacultyLogin" parent="parent">
        <item name="android:textSize">@dimen/newsMoreTextSizeFacultyLogin</item>
    </style>
    <style name="viewParentFacultyLogin">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="loginButtonFacultyLogin" parent="viewParent">
        <item name="android:background">@drawable/faculty_login_button</item>
        <item name="android:textColor">@color/whiteTextColorFacultyLogin</item>
        <item name="android:textStyle">bold</item>
    </style>
    <!--added for Forgot Password design-->
    <!--designs for login and register-->
    <style name="parentForgotPassword">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="loginParentForgotPassword" parent="parentForgotPassword">
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/loginBkColorForgotPassword</item>
        <item name="android:focusableInTouchMode">true</item>
    </style>
    <style name="headerTextForgotPassword" parent="parentForgotPassword">
        <item name="android:textSize">@dimen/headerTextSizeForgotPassword</item>
        <item name="android:textColor">@color/whiteTextColorForgotPassword</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="loginCardForgotPassword" parent="parentForgotPassword"></style>
    <style name="linearParentForgotPassword" parent="parentForgotPassword">
        <item name="android:orientation">vertical</item>
    </style>
    <style name="headerTextPrimaryForgotPassword" parent="headerTextForgotPassword">
        <item name="android:textColor">@color/primaryTextColorForgotPassword</item>
    </style>
    <style name="modifiedEditTextForgotPassword" parent="parentForgotPassword">
        <item name="android:textSize">@dimen/newsMoreTextSizeForgotPassword</item>
    </style>
    <style name="viewParentForgotPassword">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="loginButtonForgotPassword" parent="viewParentForgotPassword">
        <item name="android:background">@drawable/forgot_password_login_button_bk</item>
        <item name="android:textColor">@color/whiteTextColorForgotPassword</item>
        <item name="android:textStyle">bold</item>
    </style>

</resources>